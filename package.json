{
  "name": "recserviceswebserver",
  "version": "1.0.0",
  "description": "A modern Node.js & Express API with best practices",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"./**/*.{js,json,md}\"",
    "migrate": "npx knex migrate:latest --knexfile src/config/knexfile.js",
    "seed": "npx knex seed:run --knexfile src/config/knexfile.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "glob": "^11.0.1",
    "helmet": "^8.0.0",
    "joi": "^17.6.3",
    "morgan": "^1.10.0",
    "winston": "^3.8.2",
    "knex": "^3.1.0",
    "mysql2": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/config-array": "^0.19.2",
    "@eslint/object-schema": "^2.1.6",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "json-schema-to-ts": "^3.1.1",
    "nodemon": "^3.1.9",
    "openapi-typescript": "^7.6.1",
    "prettier": "^3.2.4",
    "redoc-express": "^2.1.0",
    "swagger-jsdoc": "^6.2.8"
  }
}
